
{
  "name": "Slack Reacjilator Demo",
  "description": "A translation bot for Slack that translates a message when a user reacted with an emoji",
  "repository": "https://github.com/slackAPI/reacjilator",
  "keywords": ["slack", "slack API", "bot", "google translate"],
  "env": {
    "SLACK_CLIENT_ID": {
      "description": "Slack Client ID",
      "value": "1234567890.1234567890"
    },
    "SLACK_CLIENT_SECRET": {
      "description": "Slack Client Secret",
      "value": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
    },
    "SLACK_VERIFICATION_TOKEN": {
      "description": "Slack Verification token",
      "value": "xxxxxxxxxxxxxxxxxxxxxxxxx"
    },
    "SLACK_AUTH_TOKEN": {
      "description": "Slack Auth token for your Slack workspace",
      "value": "xoxp-..."
    },
    "GOOGLE_PROJECT_ID": {
      "description": "Google Cloud project ID",
      "value": "myproject-123456"
    },
    "GOOGLE_KEY": {
      "description": "Google Cloud API key for your project",
      "value": "xxxxxx-xxxxxx-xxxxxx"
    }
  },
  "image": "heroku/nodejs"
}
